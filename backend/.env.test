# Test environment — safe dummy values for pytest
# This file is used automatically by pytest via pytest.ini (env_files setting)
# NEVER use these values in production

ENVIRONMENT=development
DEBUG=true

# Database — not actually connected during unit tests (SQLite in-memory used)
DATABASE_USER=test_user
DATABASE_PASSWORD=test_password_32chars_minimum_ok
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=test_db

# JWT — 64-char hex for test suite
JWT_SECRET_KEY=test_secret_key_that_is_at_least_32_characters_long_for_validation

# MQTT — not connected during unit tests
MQTT_BROKER_HOST=localhost
MQTT_USERNAME=test_mqtt_user
MQTT_PASSWORD=test_mqtt_password

# MediaMTX
STREAM_TOKEN_SECRET=test_stream_token_secret_32chars_ok

# Super admin seed
SUPER_ADMIN_USERNAME=super_admin
SUPER_ADMIN_PASSWORD=SuperAdmin@Test123
SUPER_ADMIN_EMAIL=admin@test.local
